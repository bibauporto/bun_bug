{
	"name": "opsgate",
	"private": true,
	"version": "5.8.0",
	"type": "module",
	"author": "Bashar",
	"scripts": {
		"dev": "NODE_ENV=development concurrently \"bun run dev:client\" \"bun run dev:css\" \"bun run dev:server\"",
		"dev:client": "bunx --bun vite",
		"dev:css": "sass --watch src/scss/style.scss:src/scss/style.css --no-source-map",
		"dev:server": "bun --watch run ./server/server.ts",
		"build": "NODE_ENV=production bun run build:css && bunx vite build",
		"build:css": "sass src/scss/style.scss src/scss/style.css --style compressed --no-source-map",
		"build:server": "bun build ./server/server.ts --outdir ./dist/server --target bun",
		"build:test": "NODE_ENV=test bun run build && bun run build:server",
		"build:preview": "bun run build:css && NODE_ENV=development vite build --mode development && bun run build:server",
		"compile": "bun build --compile --target=bun-linux-x64-modern ./server/server.ts --outfile ./dist/srv",
		"compile:test": "bun build --compile ./server/server.ts --outfile ./dist/srv-windows",
		"lint": "bunx eslint .",
		"lint:strict": "bunx @biomejs/biome lint .",
		"format": "bunx @biomejs/biome format . --write",
		"preview": "concurrently \"bunx vite preview\" \"bun run dev:server\"",
		"generate": "bunx typia generate --input ./server/shared/struts/templates --output ./server/shared/struts/generated --project tsconfig.json && bun run ./server/shared/struts/tsnocheck.ts",
		"check": "tsc --noEmit",
		"commit": "git add . && git commit -m 'all' && git push"
	},
	"prisma": {
		"schema": "server/prisma/schema.prisma"
	},
	"dependencies": {
		"@coreui/react": "^5.4.1",
		"@elysiajs/cors": "^1.2.0",
		"@elysiajs/cron": "^1.2.0",
		"@prisma/client": "^6.1.0",
		"@tanstack/react-query": "^5.62.8",
		"@tanstack/react-query-devtools": "^5.62.8",
		"@trpc/client": "^11.0.0-rc.666",
		"@trpc/react-query": "^11.0.0-rc.666",
		"@trpc/server": "^11.0.0-rc.666",
		"chart.js": "^4.4.7",
		"chartjs-plugin-annotation": "^3.1.0",
		"classnames": "^2.5.1",
		"dayjs": "^1.11.13",
		"elysia": "^1.2.0",
		"exceljs": "^4.4.0",
		"json2csv": "^6.0.0-alpha.2",
		"jsonwebtoken": "^9.0.2",
		"lru-cache": "^11.0.2",
		"mutative": "^1.1.0",
		"node-cache": "^5.1.2",
		"nodemailer": "^6.9.16",
		"otpauth": "^9.3.6",
		"pdf-lib": "^1.17.1",
		"primereact": "^10.8.5",
		"puppeteer": "^22.15.0",
		"rambda": "^9.4.1",
		"react": "^19.0.0",
		"react-bootstrap": "^2.10.7",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.4.0",
		"react-imask": "^7.6.1",
		"react-router-dom": "^6.28.1",
		"react-toastify": "^10.0.6",
		"simplebar": "^6.3.0",
		"simplebar-react": "^3.3.0",
		"sortablejs": "^1.15.6",
		"typia": "^7.5.1",
		"uuid": "^10.0.0",
		"zustand": "^4.5.5",
		"zustand-mutative": "^1.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@eslint/js": "^9.17.0",
		"@tanstack/eslint-plugin-query": "^5.62.1",
		"@types/bun": "^1.1.14",
		"@types/eslint__js": "^8.42.3",
		"@types/json2csv": "^5.0.7",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/nodemailer": "^6.4.17",
		"@types/react": "19.0.1",
		"@types/react-dom": "19.0.2",
		"@types/sortablejs": "^1.15.8",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"babel-plugin-react-compiler": "19.0.0-beta-201e55d-20241215",
		"concurrently": "^9.1.0",
		"eslint": "^9.17.0",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-compiler": "19.0.0-beta-201e55d-20241215",
		"globals": "^15.14.0",
		"postcss": "^8.4.49",
		"prisma": "^6.1.0",
		"prisma-json-types-generator": "^3.2.2",
		"sass": "^1.83.0",
		"tailwindcss": "^3.4.17",
		"typescript-eslint": "^8.18.1",
		"vite": "^6.0.5"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@parcel/watcher",
		"@prisma/client",
		"@prisma/engines",
		"core-js-pure",
		"esbuild",
		"prisma",
		"puppeteer"
	]
}
